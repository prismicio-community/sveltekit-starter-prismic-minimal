{
	"name": "sveltekit-starter-prismic-minimal",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "concurrently \"npm:vite:dev\" \"npm:slicemachine\" --prefix-colors blue,magenta",
		"vite:dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"slicemachine": "start-slicemachine"
	},
	"dependencies": {
		"@prismicio/client": "^7.17.2",
		"@prismicio/svelte": "^1.5.0"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.9",
		"@eslint/js": "^9.26.0",
		"@slicemachine/adapter-sveltekit": "^0.3.77",
		"@sveltejs/adapter-auto": "^6.0.1",
		"@sveltejs/kit": "^2.21.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"concurrently": "^9.1.2",
		"eslint": "^9.26.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-svelte": "^3.6.0",
		"globals": "^16.1.0",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
		"slice-machine-ui": "^2.16.0",
		"svelte": "^5.28.6",
		"svelte-check": "^4.1.7",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.32.1",
		"vite": "^6.3.5"
	}
}
